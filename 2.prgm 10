#include <iostream>
using namespace std;

int main() {
	float a,b,c,d,e,f,g;
	cout<<"subject 1="<<endl;
	cin>>a;
	cout<<"subject 2="<<endl;
	cin>>b;
	cout<<"subject 3="<<endl;
	cin>>c;
	cout<<"subject 4="<<endl;
	cin>>d;
	cout<<"subject 5="<<endl;
	cin>>e;
	f=a+b+c+d+e;
	cout<<"total mark="<<f<<endl;
	g=f/5;
	cout<<"average mark="<<g<<endl;
	int h;
	cout<<"total of maxximum mark in each subject="<<endl;
	cin>>h;
	float i;
	i=f/h*100;
	cout<<"percentage="<<i<<"%";
	return 0;
}
